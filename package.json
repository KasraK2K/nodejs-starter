{
  "name": "nodejs_starter",
  "version": "1.0.0",
  "description": "This is a node.js starter pack to create service easy and fast",
  "main": "src/index.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=production node build/index.js",
    "start:dev": "cross-env NODE_ENV=development ts-node run.ts",
    "build:dev": "rimraf build && cross-env NODE_ENV=development webpack -- --color",
    "build": "rimraf build && cross-env NODE_ENV=production webpack -- --color",
    "test": "cross-env NODE_ENV=production jest",
    "test:all": "snyk test && cross-env NODE_ENV=production jest",
    "pm2": "pm2 start ecosystem.config.js --env production",
    "pm2:dev": "pm2 start ecosystem.config.js --env development",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "docs": "rimraf docs && typedoc --options typedoc.json"
  },
  "keywords": [
    "nodejs",
    "express",
    "starter",
    "boilerplate"
  ],
  "author": "Kasra Karami <Kasra.Karami.Work@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "ajv": "^8.10.0",
    "ajv-formats": "^2.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "compression": "^1.7.4",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "cron": "^1.8.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "dotenv-expand": "^8.0.1",
    "express": "^4.17.3",
    "express-rate-limit": "^6.3.0",
    "google-libphonenumber": "^3.2.27",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "mongodb": "^4.4.1",
    "mongoose": "^6.2.3",
    "os": "^0.1.2",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/config": "^0.0.41",
    "@types/cors": "^2.8.12",
    "@types/cron": "^1.7.3",
    "@types/express": "^4.17.13",
    "@types/google-libphonenumber": "^7.4.23",
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.18",
    "@types/nodemon": "^1.19.1",
    "@types/pg": "^8.6.5",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "babel-jest": "^27.5.1",
    "gulp-typedoc": "^3.0.2",
    "jest": "^27.5.1",
    "jest-express": "^1.12.0",
    "nodemon": "^2.0.15",
    "npm": "^8.5.1",
    "snyk": "^1.869.0",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.5.5",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0",
    "webpack-shell-plugin-next": "^2.2.2"
  }
}
